{
  "name": "node-explorer",
  "private": true,
  "version": "0.1.0",
  "license": "MIT",
  "author": "Eugene Matvejev, <eugene.matvejev@gmail.com>",
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "apollo-server": "^2.6.3",
    "dataloader": "^1.4.0",
    "dotenv": "^8.0.0",
    "graphql": "^14.3.1",
    "lodash": "^4.17.11",
    "mysql2": "^1.6.5",
    "pg": "^7.11.0",
    "sequelize-cli": "^5.5.0",
    "sequelize": "^5.8.12",
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
  },
  "devDependencies": {
    "apollo-server-testing": "^2.6.7",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "sqlite3": "^4.0.9"
  },
  "main": "index.js",
  "scripts": {
    "sql": "node -r dotenv/config node_modules/.bin/babel-node ./node_modules/.bin/sequelize",
    "build": "babel --copy-files --out-dir ./build --verbose ./src",
    "start": "npm run sql db:migrate && npm run sql db:seed:all",
    "start:debug": "npm run start -- --inspect=0.0.0.0",
    "test": "node -r dotenv/config node_modules/.bin/jest"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/*/**.js",
      "!src/**?(/*)index.js?(x)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>src/jest-provision.js"
    ]
  }
}
